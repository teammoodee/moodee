<OfficeApp xsi:type="MailApp" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/office/appforoffice/1.0">
  <Id>c3d7ecd3-9602-42d5-8c33-39710c0d3235</Id>
  <Version>1.0.0.1</Version>
  <ProviderName>Moodee Productions</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Moodee" />
  <Description DefaultValue="R U Moodee" />
  <IconUrl DefaultValue="https://moodee.azurewebsites.net/red_smiley_64x64.png" />
  <SupportUrl DefaultValue="https://moodee.azurewebsites.net/support.html" />
  <Capabilities>
    <Capability Name="Mailbox" />
  </Capabilities>
  <DesktopSettings>
    <SourceLocation DefaultValue="https://moodee.azurewebsites.net" />
    <RequestedHeight>200</RequestedHeight>
  </DesktopSettings>
  <TabletSettings>
    <SourceLocation DefaultValue="https://moodee.azurewebsites.net" />
    <RequestedHeight>200</RequestedHeight>
  </TabletSettings>
  <PhoneSettings>
    <SourceLocation DefaultValue="https://moodee.azurewebsites.net" />
  </PhoneSettings>
  <Permissions>ReadItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="And">
    <!-- To add more complex rules, add additional rule elements -->
    <Rule xsi:type="ItemIs" ItemType="Message" />
    <!-- E.g. To activate when a message contains an address -->
    <!-- <Rule xsi:type="ItemHasKnownEntity" EntityType="Address" /> -->
  </Rule>
</OfficeApp>